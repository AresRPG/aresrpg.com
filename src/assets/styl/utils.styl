reset()
    margin 0
    padding 0
    box-sizing border-box

disableScollBar() 
    ::-webkit-scrollbar 
        display: none;

.viewPortVideo
  position absolute
  top 50%
  left 50%
  -webkit-transform translateX(-50%) translateY(-50%)
  transform translateX(-50%) translateY(-50%)
  min-width 100%
  min-height 100%
  width auto
  height auto
  z-index -1000
  overflow hidden